{
    "manifest_version": 2,
    "name": "Rest Reminder",
    "description": "Nudges you to rest your eyes in customizable intervals",
    "version": "1.0.0",
    "icons": {"128": "icon_128.png"},
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": ["tabs", "storage", "alarms"]
}